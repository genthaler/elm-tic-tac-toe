{
  "name": "elm-tic-tac-toe",
  "version": "1.0.0",
  "description": "Tic-tac-toe with some AI",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "justtest": "elm-test",
    "pretest": "elm-verify-examples",
    "test": "elm-test",
    "verify": "elm-verify-examples --run-tests",
    "posttest": "shx rm -rf tests/VerifyExamples",
    "start": "parcel index.html",
    "predeploy": "parcel build --public-url=. index.html",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genthaler/elm-tic-tac-toe.git"
  },
  "keywords": [
    "elm",
    "game",
    "search",
    "ai"
  ],
  "author": "genthaler",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/genthaler/elm-tic-tac-toe/issues"
  },
  "homepage": "https://github.com/genthaler/elm-tic-tac-toe#readme",
  "devDependencies": {
    "@parcel/transformer-elm": "^2.8.3",
    "elm": "0.19.1-5",
    "elm-review": "^2.9.2",
    "elm-test": "^0.19.1-revision7",
    "elm-verify-examples": "^5.0.0",
    "gh-pages": "^3.2.3",
    "parcel": "^2.0.0",
    "shx": "^0.3.3"
  }
}
