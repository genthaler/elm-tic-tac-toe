{
  "name": "elm-tic-tac-toe",
  "version": "1.0.0",
  "description": "My homage to the hex-based strategy games of yesterday",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "justtest": "elm-test",
    "pretest": "elm-format . --yes && elm-verify-examples",
    "test": "elm-test",
    "posttest": "shx rm -rf tests/VerifyExamples",
    "start": "parcel index.html",
    "predeploy": "parcel build --public-url=. index.html",
    "deploy": "gh-pages -d dist",
    "broken": "elm-xref src/Main.elm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genthaler/elm-tic-tac-toe.git"
  },
  "keywords": [
    "elm",
    "hex",
    "grid",
    "war"
  ],
  "author": "genthaler",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/genthaler/elm-tic-tac-toe/issues"
  },
  "homepage": "https://github.com/genthaler/elm-tic-tac-toe#readme",
  "dependencies": {},
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.2",
    "elm-hot": "^1.1.3",
    "elm-test": "^0.19.1",
    "elm-verify-examples": "^5.0.0",
    "gh-pages": "^2.1.1",
    "parcel-bundler": "^1.12.4",
    "shx": "^0.3.2"
  }
}