{
  "name": "elm-tic-tac-toe",
  "version": "1.0.0",
  "description": "Tic-tac-toe with some AI",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "clean": "shx rm -rf .parcel-cache ./dist ./index.html",
    "parcel": "parcel src/index.html",
    "book": "elm-book src/Book.elm",
    "prebuild": "npm run clean",
    "build": "parcel build src/index.html --no-source-maps --public-url ./",
    "preserve": "npm run build",
    "serve": "serve dist --listen 3000",
    "review": "elm-review",
    "review:fix": "elm-review --fix-all-without-prompt",
    "review:perf": "time elm-review --benchmark-info",
    "review:ci": "elm-review --no-color --report=json",
    "pretest": "elm-verify-examples",
    "test": "elm-test",
    "posttest": "shx rm -rf tests/VerifyExamples",
    "predeploy": "npm run test && npm run review && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genthaler/elm-tic-tac-toe.git"
  },
  "keywords": [
    "elm",
    "game",
    "search",
    "ai"
  ],
  "author": "genthaler",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/genthaler/elm-tic-tac-toe/issues"
  },
  "homepage": "https://github.com/genthaler/elm-tic-tac-toe#readme",
  "devDependencies": {
    "@parcel/transformer-elm": "^2.16.0",
    "elm": "^0.19.1-3",
    "elm-book": "^1.0.1",
    "elm-review": "^2.13.3",
    "elm-test": "^0.19.1-revision17",
    "elm-verify-examples": "^6.0.3",
    "gh-pages": "^6.3.0",
    "parcel": "^2.12.0",
    "serve": "^14.2.5",
    "shx": "^0.4.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 20"
  }
}