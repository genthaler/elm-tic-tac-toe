{
  "name": "elm-tic-tac-toe",
  "version": "1.0.0",
  "description": "Tic-tac-toe with some AI",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "parcel": "parcel src/index.html",
    "book": "elm-book src/Book.elm",
    "prebuild": "shx rm -rf .parcel-cache dist",
    "build": "parcel build --no-source-maps --public-url ./",
    "preserve": "npm run build",
    "serve": "serve dist",
    "review": "elm-review",
    "review:fix": "elm-review --fix-all-without-prompt",
    "review:perf": "time elm-review --benchmark-info",
    "review:clean": "elm-review --ignore-dirs tests/",
    "review:ci": "elm-review --no-color --report=json",
    "pretest": "elm-verify-examples && npm run review",
    "test": "elm-test",
    "posttest": "shx rm -rf tests/VerifyExamples",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genthaler/elm-tic-tac-toe.git"
  },
  "keywords": [
    "elm",
    "game",
    "search",
    "ai"
  ],
  "author": "genthaler",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/genthaler/elm-tic-tac-toe/issues"
  },
  "homepage": "https://github.com/genthaler/elm-tic-tac-toe#readme",
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-hot": "^1.1.3",
    "elm-review": "^2.10.3",
    "elm-verify-examples": "^5.0.0",
    "elm-test": "^0.19.1",
    "gh-pages": "^5.0.0",
    "parcel-bundler": "^1.12.4",
    "shx": "^0.3.2"
  },
  "source": [
    "src/index.html",
    "src/Book.elm"
  ],
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 20"
  }
}